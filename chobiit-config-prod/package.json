  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build:jp:dev": "SYSTEM_ENV=dev dotenv -e ./../.env.jp.dev -- ts-node scripts/build.ts",
    "build:us:dev": "SYSTEM_ENV=dev dotenv -e ./../.env.us.dev -- ts-node scripts/build.ts",
    "build:jp:prod": "SYSTEM_ENV=prod dotenv -e ./../.env.jp.prod -- ts-node scripts/build.ts",
    "build:us:prod": "SYSTEM_ENV=prod dotenv -e ./../.env.us.prod -- ts-node scripts/build.ts",
    "deploy:jp:dev": "npm run build:jp:dev && SYSTEM_ENV=dev dotenv -e ./../.env.jp.dev -- ts-node scripts/deploy.ts",
    "deploy:us:dev": "npm run build:us:dev && SYSTEM_ENV=dev dotenv -e ./../.env.us.dev -- ts-node scripts/deploy.ts",
    "deploy:jp:prod": "npm run build:jp:prod && SYSTEM_ENV=prod dotenv -e ./../.env.jp.prod -- ts-node scripts/deploy.ts",
    "deploy:us:prod": "npm run build:us:prod && SYSTEM_ENV=prod dotenv -e ./../.env.us.prod -- ts-node scripts/deploy.ts",
    "package:jp:dev": "./scripts/package.sh dev jp",
    "package:us:dev": "./scripts/package.sh dev us",
    "package:jp:prod": "./scripts/package.sh prod jp",
    "package:us:prod": "./scripts/package.sh prod us",
    "test": "jest"
  },
  "keywords": [],

    "@types/prompts": "^2.4.4",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "esbuild": "^0.18.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.1.1",
    "prompts": "^2.4.2",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^5.0.5",

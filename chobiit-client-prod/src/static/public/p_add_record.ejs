<!DOCTYPE html>
<html lang="<%= chobiitLang %>">

<head>
    <meta charset="UTF-8">
    <meta name="robots" content="noindex">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Chobiit</title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" >
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/"
        crossorigin="anonymous">
    <link rel="stylesheet" href="../css/main.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.css">
    <style>
        #iframe {
            width: 100%;
            height: 90vh;
            border: none;
        }
        nav {
            padding-top: 70px;
        }

        @media screen and (max-width: 767.98px) {
            nav {
                padding-top: 0px;
            }
        }

    </style>
    <script src="../js/config.js"></script>
</head>

<body>
    <div id="loader" class="se-pre-con" style="display: none"></div>
        <div id="content">
            <!-- Header -->

            <div class="col-md-12 px-0 pt-2" id="mainContainer">
                <!-- <p class="d-sm-block d-md-none">
                    <button type="button" class="btn btn-outline-info btn-sm" onclick="toggleClass(document.getElementById('rowOffCanvas'), 'active')">
                        <i class="fas fa-angle-left"></i>
                    </button>
                </p> -->

                <nav aria-label="breadcrumb">
                </nav>

                <!-- Content -->
                <div id="mainContent">
                    <div class="d-flex flex-wrap justify-content-between follow" style="top: 0; left: 0">
                        <div class="edit-menu" id="editMenu" style="visibility: visible; margin-top: 15px; margin-bottom: 15px; margin-left: 15px ">
                            <button type="button" class="btn btn-cancel mr-1" id="cancelBtn"><%= message.common["cancel-title"] %></button>
                            <button type="button" class="btn btn-green mr-1" id="submitBtn" disabled ><%= message.common["save-title"] %></button>
                        </div>
                        <div style="height: 100px;">
                            <div id="recapcha" style="float:left;margin-right: 5px; padding-left: 15px;"></div>
                        </div>
                    </div>
                    <iframe id="iframe" src=""></iframe>
                    <br>
                </div>
                <!-- End content -->
            </div>
        </div>

    <!-- recapcha -->
    <script type="text/javascript">
        var submitBtn = document.getElementById('submitBtn');
        var captchaCallback = function () {
            submitBtn.removeAttribute('disabled');
        };

        var captchaExpiredCallback = function () {
            submitBtn.setAttribute('disabled', true);
        };

        var onloadCallback = function () {
            grecaptcha.render('recapcha', {
                'sitekey': '6LdUspkUAAAAAA1_t85vnfCWCf5Wlz1XuoiipbCP',
                'callback': captchaCallback,
                'expired-callback': captchaExpiredCallback        
            });
        };
    </script>
    <!-- <script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit" async defer> -->
    </script>
    <script src="https://code.jquery.com/jquery-3.3.1.js" integrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60="
    crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49"
        crossorigin="anonymous"></script>
      <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.4/jquery-confirm.min.js"></script>
    <script src="https://js.cybozu.com/sweetalert/v2.1.2/sweetalert.min.js"></script>
    <script src="../js/main.js"></script>
    <script src="../js/p-add-record.js"></script>
    <script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit" async defer></script>
</body>

</html>
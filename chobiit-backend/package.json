    "build:dev": "SYSTEM_ENV=dev node --require esbuild-register scripts/build.ts",
    "build:prod": "SYSTEM_ENV=prod node --require esbuild-register scripts/build.ts",
    "deploy:jp:dev": "SYSTEM_ENV=dev dotenv -e ./../.env.jp.dev -- node --require esbuild-register scripts/deploy.ts",
    "deploy:us:dev": "SYSTEM_ENV=dev dotenv -e ./../.env.us.dev -- node --require esbuild-register scripts/deploy.ts",
    "deploy:jp:prod": "SYSTEM_ENV=prod dotenv -e ./../.env.jp.prod -- node --require esbuild-register scripts/deploy.ts",

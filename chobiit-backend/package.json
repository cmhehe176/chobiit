    "set-common-envs:us:dev": "SYSTEM_ENV=dev dotenv -e ./../.env.us.dev -- node --require esbuild-register scripts/set-common-environment.ts",
    "set-common-envs:jp:prod": "SYSTEM_ENV=prod dotenv -e ./../.env.jp.prod -- node --require esbuild-register scripts/set-common-environment.ts",
